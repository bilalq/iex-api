{
  "name": "iex-luxcium-api",
  "version": "0.0.5",
  "description": "Unofficial SDK for using the IEX API by Luxcium.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc",
    "lint": "tslint --project . --format stylish",
    "pretest": "tsc --noEmit --pretty && npm run lint",
    "test": "jest",
    "gitcheck": "git diff-index   origin/master",
    "copystatic": "cp package.json .npmignore LICENSE README.md build/",
    "predist": "npm run gitcheck && npm run test && npm run build && npm run copystatic",
    "dist": "cd build && npm publish",
    "prepublishOnly": "[[ \"$PWD\" =~ build ]]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxcium/iex-api.git"
  },
  "keywords": [
    "iex",
    "trading",
    "stock",
    "market",
    "api",
    "sdk"
  ],
  "author": "Benjamin Vincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luxcium/iex-api/issues"
  },
  "homepage": "https://github.com/luxcium/iex-api#readme",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/apis/**/*.ts"
    ],
    "coverageDirectory": "build/coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules|build/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@types/inquirer": "0.0.44",
    "@types/node-fetch": "^2.1.6",
    "inquirer": "^6.2.2",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "rxjs": "^6.2.2",
    "socket.io-client": "^2.1.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333",
    "typescript-tslint-plugin": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/npm": "^2.0.29",
    "@types/socket.io-client": "^1.4.32",
    "fetch-ponyfill": "^4.1.0",
    "jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.8.0"
  }
}
